{
    "name": "rizalfauzie/sellerapi",
    "description": "Udropship Seller Open API.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/redis": "^9.23",
        "laravel/lumen-framework": "^9.0",
        "league/flysystem": "^3.0",
        "predis/predis": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
